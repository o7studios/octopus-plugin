{
  "name": "Octopus Plugin",
  "image": "ubuntu:latest",
  "customizations" : {
    "jetbrains" : {
      "backend" : "IntelliJ",
      "plugins": [
        "com.demonwav.minecraft-dev"
      ]
    }
  },
  "features": {
    "ghcr.io/devcontainers/features/git" : {},
    "ghcr.io/devcontainers/features/java:1": {
      "version": "23",
      "installGradle": true
    }
  },
  "runArgs": [
    "--env-file=${localEnv:HOME}/dev.env"
  ],
  "remoteUser": "ubuntu"
}
